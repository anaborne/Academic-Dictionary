{
    "manifest_version": 3,
    "name": "Academic Dictionary",
    "version": "1.0",
    "permissions": [
      "contextMenus",
      "storage",
      "tabs"
    ],
   "host_permissions": [
     "https://api.openai.com/*"
  ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }